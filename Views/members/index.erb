<h1 style="text-align:center;">Members.</h1>
<% for member in @members %>
  <br>
  <h3 style="text-align:center;"> Member name: <%= member.first_name %> </h3>
  <h4 style="text-align:center;"> <a href="/members/<%= member.id %>/members"> </a> </h4>

  <form action="/members/<%= member.id %>/delete" method="POST">
    <input type="submit" value="delete" />
  </form>
  <form action="/members/<%= member.id %>/edit" method="POST">
    <input type="submit" value="edit" />
  </form>

  <br>
  <hr>
<% end %>
