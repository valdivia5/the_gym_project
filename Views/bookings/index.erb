<h1>Bookings</h1>

<p
  <a  href="../new">New booking!</a>
</p>

<ul>
  <% for booking in @bookings %>
  <li>
  <p><%= booking.member.first_name %> <%= booking.activity.name %> </p>
    <p>
      <form action="/bookings/<%= booking.id %>/delete" method="POST">
        <input type="submit" value="delete" />
      </form>
    </p>
  </li>
  <% end %>
</ul>
